{
    "name": "rollerworks/version",
    "description": "Semantic versioning helper library",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Sebastiaan Stok",
            "email": "s.stok@rollerscapes.net"
        }
    ],
    "require": {
        "php": "^8.1"
    },
     "require-dev": {
         "phpunit/phpunit": "^10.5 || ^11.0",
         "rollerscapes/standards": "^0.3"
     },
    "autoload": {
        "psr-4": {
            "Rollerworks\\Component\\Version\\": "src/"
        },
        "exclude-from-classmap": [
            "tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Rollerworks\\Component\\Version\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.0-dev"
        }
    }
}
