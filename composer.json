{
    "name": "rollerworks/version",
    "description": "Semantic versioning helper library",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Sebastiaan Stok",
            "email": "s.stok@rollerscapes.net"
        }
    ],
    "require": {
        "php": "^7.1|^8.0"
    },
     "require-dev": {
         "phpunit/phpunit": "^7.3.2|^8.5.12",
         "phpstan/phpstan": "^0.12.33",
         "phpstan/phpstan-phpunit": "^0.12.13",
         "symfony/phpunit-bridge": "^5.2"
     },
    "autoload": {
        "psr-4": {
            "Rollerworks\\Component\\Version\\": "src/"
        },
        "exclude-from-classmap": [
            "tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Rollerworks\\Component\\Version\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.2-dev"
        }
    }
}
